<apex:page controller="ProcessSelectedAccounts" showHeader="false" sidebar="false">
<apex:form >
<apex:pageBlock >
    <apex:pageBlockButtons >
            <apex:commandButton value="Process Selected" action="{!processselected}" />
    </apex:pageBlockButtons>
    <apex:pageBlockSection title="All Accounts">
       <apex:pageBlockTable value="{!listWrapAcc}" var="wrapacc">
           <apex:column value="{!wrapacc.acc.name}" headerValue="Account Name"/>
           <apex:column value="{!wrapacc.acc.phone}" headerValue="Phone"/>
           <apex:column value="{!wrapacc.acc.billingstate}" headerValue="Billing Address"/>
           <apex:column headerValue="Select Record">
           		<apex:inputCheckbox value="{!wrapacc.sel}"/>
           </apex:column>
       </apex:pageBlockTable>
    </apex:pageBlockSection>
    <apex:pageblockSection title="Selected Account">
        <apex:pageBlockTable value="{!wrapac}" var="w">
        	<apex:column value="{!w.name}" headerValue="Account Name"/>
        </apex:pageBlockTable>
    </apex:pageblockSection>
</apex:pageBlock>    
</apex:form>    

</apex:page>