<apex:page standardController="Merchandise__c" recordSetVar="merchandise" showHeader="false" sidebar="false">
 <apex:form >
  <apex:pageBlock title="Merchandise Detail Page">
  <apex:pageMessages />
   <apex:pageBlockButtons >
    <apex:commandButton value="Save" action="{!save}"/>
   </apex:pageBlockButtons>
   <apex:pageBlockTable value="{!merchandise}" var="m">
     <apex:column value="{!m.name}"/>
     <apex:column headerValue="Price">
      <apex:inputField value="{!m.Price__c}"/>
     </apex:column>
     <apex:column headervalue="Quantity">
      <apex:inputField value="{!m.Quantity__c}"/>
     </apex:column>
     
   </apex:pageBlockTable>
   
  </apex:pageBlock>
 </apex:form>
</apex:page>