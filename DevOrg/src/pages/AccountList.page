<apex:page standardController="Account" recordSetVar="accounts" showHeader="false" sidebar="false" docType="html-5.0">
  <apex:form >
    <apex:pageBlock title="Account Details">
       <apex:pageBlockSection >
          <apex:repeat value="{!accounts}" var="a">
              <li>
              <apex:outputLink value="/{!a.id}">{!a.Name}</apex:outputLink>
              </li>
          </apex:repeat>
       </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>