    <validationRules>
        <fullName>Creation_Rule</fullName>
        <active>true</active>
        <description>This will not any new field to be created</description>
        <errorConditionFormula>AND(NOT(ISNEW()), Application_Cost__c &lt; 200)</errorConditionFormula>
        <errorMessage>Application Cost can not be less than 200 only for newly created Applications</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Postal_Code</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR( Country__c  = &quot;INDIA&quot;,  Country__c  = &quot;IN&quot;),
NOT(REGEX( Zip_Postal_Code__c ,
&quot;((?i)[A-Z]\\d[A-Z]?\\s?\\d[A-Z]\\d)?&quot;))
)</errorConditionFormula>
        <errorMessage>Canadian postal code must be in A9A 9A9 format.</errorMessage>
    </validationRules>
